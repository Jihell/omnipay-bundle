{
    "name": "colinodell/omnipay-bundle",
    "description": "Omnipay bundle for Symfony 2.3+ and 3.0+",
    "keywords": ["omnipay", "bundle", "symfony", "payment", "symfony2", "symfony3"],
    "homepage": "https://github.com/colinodell/omnipay-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "colinodell@gmail.com",
            "homepage": "https://www.colinodell.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "omnipay/common": "^3.0",
        "symfony/framework-bundle": "^2.3||^3.0"
    },
    "require-dev": {
        "omnipay/paypal": "^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "^3.7|^4.8"
    },
    "autoload": {
        "psr-4": {
            "ColinODell\\OmnipayBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
